
@{
    Layout = null;
}

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="assets/img/apple-icon.png">
    <link rel="icon" type="image/png" href="assets/img/favicon.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Get Shit Done Bootstrap Wizard by Creative Tim</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


    <!-- CSS Files -->
    <link href="~/Content/css/UserData/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/UserData/gsdk-bootstrap-wizard.css" rel="stylesheet" />

    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="~/Content/css/UserData/demo.css" rel="stylesheet" />
</head>

<body>
    <div class="image-container set-full-height" style="background-image: url('/Images/UserData/beijing.jpg')">
       
        <!--  Made With Get Shit Done Kit  -->
        <a href="#/get-shit-done/index.html?ref=get-shit-done-bootstrap-wizard" class="made-with-mk">
            <div class="brand">QQ</div>
            <div class="made-with">G疯的群:<strong>565732304</strong></div>
        </a>

        <!--   Big container   -->
        <div class="container">
            <div class="row">
                <div class="col-sm-8 col-sm-offset-2">

                    <!--      Wizard container        -->
                    <div class="wizard-container">

                        <div class="card wizard-card" data-color="orange" id="wizardProfile">
                            <form action="" method="">
                                <!--        You can switch ' data-color="orange" '  with one of the next bright colors: "blue", "green", "orange", "red"          -->

                                <div class="wizard-header">
                                    <h3>
                                        <b>G疯</b> 疯子们的游戏世界 <br>
                                        <small>在此之前，请填写一些关于您的爱好吧，以便于我们更好的为您提供服务.</small>
                                    </h3>
                                </div>

                                <div class="wizard-navigation">
                                    <ul>
                                        <li><a href="#about" data-toggle="tab">我及游戏</a></li>
                                        <li><a href="#account" data-toggle="tab">我及差事</a></li>
                                        <li><a href="#address" data-toggle="tab">我及我们</a></li>
                                    </ul>

                                </div>

                                <div class="tab-content">
                                    <div class="tab-pane" id="about">
                                        <div class="row">
                                            <h4 class="info-text">请在此处填写一些你喜欢的游戏类型和你的联系方式</h4>
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <div class="form-group">
                                                    <label>wo喜欢的游戏类型</label><br>
                                                    <select name="country" class="form-control">
                                                        <option value="FPS"> FPS-第一人称射击 </option>
                                                        <option value="FTG"> FTG-格斗 </option>
                                                        <option value="RAC"> RAC-竞赛 </option>
                                                        <option value="RPG"> RPG-角色扮演 </option>
                                                        <option value="RTS"> RTS-即时战略 </option>
                                                        <option value="MUG"> MUG-音乐 </option>
                                                        <option value="SPG"> SPG-运动 </option>
                                                        <option value="ETC"> ETC-其他 </option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label>wo在线时间</label><br>
                                                    <select name="country" class="form-control">
                                                        <option value="Morning"> 上午 </option>
                                                        <option value="Noon"> 中午 </option>
                                                        <option value="Afternoon"> 下午 </option>
                                                        <option value="Night"> 晚上 </option>
                                                        <option value="Latenight"> 深夜 </option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label>wo的姓氏<small>(英文名或汉语拼音)</small></label>
                                                    <input name="firstname" type="text" class="form-control" placeholder="Zhang">
                                                </div>
                                                <div class="form-group">
                                                    <label>wo的名字<small>(英文名或汉语拼音)</small></label>
                                                    <input name="lastname" type="text" class="form-control" placeholder="San">
                                                </div>
                                            </div>
                                            <div class="col-sm-4 col-sm-offset-1">
                                                <div class="form-group">
                                                    <label>wo的年龄</label><br>
                                                    <select name="country" class="form-control">
                                                        <option value="10"> 10岁以下 </option>
                                                        <option value="25"> 11~25岁 </option>
                                                        <option value="40"> 26~40岁 </option>
                                                        <option value="55"> 41~55岁 </option>
                                                        <option value="70"> 56~70岁 </option>
                                                        <option value="71"> 71岁以上 </option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div>
                                                <div class="col-sm-6">


                                                </div>
                                            </div>
                                            <div class="col-sm-10 col-sm-offset-1">
                                                <div class="form-group">
                                                    <label>Email <small>(字母或数字的组合)</small></label>
                                                    <input name="email" type="email" class="form-control" placeholder="Gamesfeng@gserver.com">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="tab-pane" id="account">
                                        <h4 class="info-text"> 你的工作性质是什么? (可多选) </h4>
                                        <div class="row">

                                            <div class="col-sm-10 col-sm-offset-1">
                                                <div class="col-sm-3">
                                                    <div class="choice" data-toggle="wizard-checkbox">
                                                        <input type="checkbox" name="jobb" value="Design">
                                                        <div class="icon">
                                                            <i class="fa fa-pencil"></i>
                                                        </div>
                                                        <h6>设计</h6>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="choice" data-toggle="wizard-checkbox">
                                                        <input type="checkbox" name="jobb" value="Design">
                                                        <div class="icon">
                                                            <i class="fa fa-pencil"></i>
                                                        </div>
                                                        <h6>教育</h6>
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="choice" data-toggle="wizard-checkbox">
                                                        <input type="checkbox" name="jobb" value="Code">
                                                        <div class="icon">
                                                            <i class="fa fa-terminal"></i>
                                                        </div>
                                                        <h6>开发</h6>
                                                    </div>

                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="choice" data-toggle="wizard-checkbox">
                                                        <input type="checkbox" name="jobb" value="Develop">
                                                        <div class="icon">
                                                            <i class="fa fa-laptop"></i>
                                                        </div>
                                                        <h6>建筑</h6>
                                                    </div>

                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="tab-pane" id="address">
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <h4 class="info-text"> Are you living in a nice area? </h4>
                                            </div>
                                            <div class="col-sm-6 col-sm-offset-1">
                                                <div class="form-group">
                                                    <label>您的省市 (选填)</label>
                                                    <input type="text" class="form-control" placeholder="甘肃省-酒泉市">
                                                </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label>您的QQ号</label>
                                                    <input type="text" class="form-control" placeholder="31141612001">
                                                </div>
                                            </div>
                                            <div class="col-sm-5 col-sm-offset-1">
                                                <div class="form-group">
                                                    <label>您对我们的期望</label>
                                                    <input type="text" class="form-control" placeholder="我希望你们XXXX....">
                                                </div>
                                            </div>
                                            <div class="col-sm-5">
                                                <div class="form-group">
                                                    <label>我和游戏</label><br>
                                                    <select name="country" class="form-control">
                                                        <option value="Afghanistan"> 偶尔自己娱乐 </option>
                                                        <option value="Albania"> 经常和队友娱乐 </option>
                                                        <option value="Algeria"> 每天都和朋友玩 </option>
                                                        <option value="American Samoa"> 职业游戏选手 </option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="wizard-footer height-wizard">
                                    <div class="pull-right">
                                        <input type='button' class='btn btn-next btn-fill btn-warning btn-wd btn-sm' name='next' value='下一个' />
                                        <input type='button' class='btn btn-finish btn-fill btn-warning btn-wd btn-sm' name='finish' value='提交' />

                                    </div>

                                    <div class="pull-left">
                                        <input type='button' class='btn btn-previous btn-fill btn-default btn-wd btn-sm' name='previous' value='上一个' />
                                    </div>
                                    <div class="clearfix"></div>
                                </div>

                            </form>
                        </div>
                    </div> <!-- wizard container -->
                </div>
            </div><!-- end row -->
        </div> <!--  big container -->

        <div class="footer">
            <div class="container">
                made with <i class="fa fa-heart heart"></i> by <a href="#">Creative Tim</a>. More Templates <a href="http://www.cssmoban.com/" target="_blank" title="模板之家">模板之家</a> - Collect from <a href="http://www.cssmoban.com/" title="网页模板" target="_blank">网页模板</a>
            </div>
        </div>

    </div>

</body>

<!--   Core JS Files   -->
<script src="~/Content/js/UserData/jquery-2.2.4.min.js" type="text/javascript"></script>
<script src="~/Content/js/UserData/bootstrap.min.js" type="text/javascript"></script>
<script src="~/Content/js/UserData/jquery.bootstrap.wizard.js" type="text/javascript"></script>

<!--  Plugin for the Wizard -->
<script src="~/Content/js/UserData/gsdk-bootstrap-wizard.js"></script>

<script src="~/Content/js/UserData/jquery.validate.min.js"></script>

</html>

